    "@types/react-timeago": "^4.1.3",
    "autoprefixer": "^10.0.2",
    "fs-extra": "^10.0.1",
    "inquirer": "^8.2.2",
    "klaw": "^4.0.1",
    "postcss": "^8.1.0",
